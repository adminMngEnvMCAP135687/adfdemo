{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfworkshopSG"
		},
		"LS_BLOB_srcviguyonv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_srcviguyonv'"
		},
		"LS_BLOB_SNCF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_SNCF'"
		},
		"LS_BLOB_VELIB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_VELIB'"
		},
		"ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://penduickadls.dfs.core.windows.net/"
		},
		"LS_VELIB_STATION_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat('https://velib-metropole-opendata.smoove.pro/opendata/Velib_Metropole/',linkedService().p_api_extension)}"
		},
		"HOURLY_TRIGGER_properties_GET VELIB DYNAMIC_parameters_subject": {
			"type": "string",
			"defaultValue": "information"
		},
		"2MINUTE_TRIGGER_properties_GET VELIB DYNAMIC_parameters_subject": {
			"type": "string",
			"defaultValue": "status"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfworkshopSG_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BLOB_srcviguyonv_connectionString": {
						"value": "[parameters('LS_BLOB_srcviguyonv_connectionString')]"
					},
					"LS_BLOB_SNCF_connectionString": {
						"value": "[parameters('LS_BLOB_SNCF_connectionString')]"
					},
					"LS_BLOB_VELIB_connectionString": {
						"value": "[parameters('LS_BLOB_VELIB_connectionString')]"
					},
					"ADLS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_properties_typeProperties_url')]"
					},
					"LS_VELIB_STATION_properties_typeProperties_url": {
						"value": "[parameters('LS_VELIB_STATION_properties_typeProperties_url')]"
					},
					"HOURLY_TRIGGER_properties_GET VELIB DYNAMIC_parameters_subject": {
						"value": "[parameters('HOURLY_TRIGGER_properties_GET VELIB DYNAMIC_parameters_subject')]"
					},
					"2MINUTE_TRIGGER_properties_GET VELIB DYNAMIC_parameters_subject": {
						"value": "[parameters('2MINUTE_TRIGGER_properties_GET VELIB DYNAMIC_parameters_subject')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}